{
  "name": "ChatGPT Folders",
  "version": "0.0.0",
  "description": "ChatGPT folders thread name. Indicate the folder name with parentheses.",
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab",
    "storage"
  ],
  "content_scripts": [{
    "matches": ["https://chat.openai.com/"],
    "js": ["js/content.js"],
    "css": ["css/styles.css"],
    "run_at": "document_end"
  }],
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/icon.png",
      "48": "img/icon.png",
      "128": "img/icon.png"
    }
  },
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  }
}
